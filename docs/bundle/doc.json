    "/auth/register": {
      "post": {
        "tags": [
          "auth"
        ],
        "summary": "Register and get token",
        "description": "Register user and get auth token and user doc",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "name",
                  "username",
                  "password",
                  "passwordConfirmation",
                  "email"
                ],
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "username": {
                    "type": "string",
                    "minLength": 4
                  },
                  "password": {
                    "type": "string",
                    "minLength": 4
                  },
                  "passwordConfirmation": {
                    "type": "string",
                    "minLength": 4
                  },
                  "email": {
                    "type": "string",
                    "format": "email"
                  },
                  "role": {
                    "type": "string",
                    "enum": [
                      "user",
                      "seller"
                    ]
                  },
                  "address": {
                    "type": "string"
                  },
                  "phone": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "User registered Successfuly",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/paths/~1auth~1login/post/responses/200/content/application~1json/schema"
                }
              }
            }
          },
          "400": {
            "description": "Faild to register bad request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/paths/~1user/get/responses/401/content/application~1json/schema"
                }
              }
            }
          }
        }
      }
    },
